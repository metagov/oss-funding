          "fundsApprovedInUSD": 15000,
          "title": "IDUNU: Help Kids Thrive",
          "description": "IDUNU: A fundraising and fund distribution platform for high-impact, cost-effective charities working with underprivileged kids.",
          "fundsPaid": [
            {
              "amount": 15000.0,
              "denomination": "USD"
            },
            {
              "amount": 112832.0,
              "denomination": "XLM"
