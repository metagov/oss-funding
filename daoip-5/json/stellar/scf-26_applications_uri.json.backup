          "projectId": "daoip5:stellar:project:clear",
          "projectName": "Clear - Liquidity for payments",
          "createdAt": "2024-04-24T00:00:00Z",
          "contentURI": "https://airtable.com/appymB1sbp5uidiGe/shrsaWm1IyNZTCEhX/tblpytbu3t73kssiE/viwTba5X3EUwzXlcr/recZI6G0tdDrevSQz",
